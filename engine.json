{
    "name": "DeepL DIY",
    "languages": {
        "source": {
            "Source Language": "AUTO"
        },
        "target": {
            "Target Language": "ZH"
        }
    },
    "request": {
        "url": "http://127.0.0.1:9528/translate",
        "method": "POST",
        "headers": {
            "Content-Type": "application/json"
        },
        "data": {
            "source_lang": "<source>",
            "target_lang": "<target>",
            "text": "<text>"
        }
    },
    "response": "response['result']['texts'][0]['text']"
}